{
  "kind": "collectionType",
  "collectionName": "tenant_guides",
  "info": {
    "singularName": "tenant-guide",
    "pluralName": "tenant-guides",
    "displayName": "Guide",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "question": {
      "type": "string",
      "required": true
    },
    "answer": {
      "type": "blocks",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "tenants",
        "landlords",
        "agencies",
        "general"
      ],
      "default": "general",
      "required": true
    },
    "country": {
      "type": "customField",
      "customField": "plugin::country-select.country"
    }
  }
}
